<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Zad3="clr-namespace:Zad3.Converters">

    <SolidColorBrush x:Key="squareDisabled" Color="Beige"></SolidColorBrush>
    <SolidColorBrush x:Key="squareEnabled" Color="Black"></SolidColorBrush>


    <Zad3:BooleanToBrushConverter x:Key="BooleanToBrushConverter" TrueBrush="{StaticResource squareEnabled}" FalseBrush="{StaticResource squareDisabled}" />
    <Zad3:BooleanToYesNoConverter x:Key="BooleanToYesNoConverter" TrueString="tak" FalseString="nie" />

</ResourceDictionary>