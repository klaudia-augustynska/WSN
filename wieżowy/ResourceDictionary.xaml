<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pawelek="clr-namespace:pawelek.Converters">

    <SolidColorBrush x:Key="squareDisabled" Color="Beige"></SolidColorBrush>
    <SolidColorBrush x:Key="squareEnabled" Color="Black"></SolidColorBrush>


    <pawelek:BooleanToBrushConverter x:Key="BooleanToBrushConverter" TrueBrush="{StaticResource squareEnabled}" FalseBrush="{StaticResource squareDisabled}" />
    <pawelek:BooleanToYesNoConverter x:Key="BooleanToYesNoConverter" TrueString="tak" FalseString="nie" />

</ResourceDictionary>